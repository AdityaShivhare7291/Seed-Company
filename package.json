{
  "name": "new-folder",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "build": "electron-builder",
    "creator":"yarn electron-builder --prepackaged ./dist/win-unpacked  --win"
  },
  "build": {
    "appId": "com.myapp.id",
    "productName": "Inventory",
    "files": [
      "**/build/**/*",
      "**/node_modules/**/*",
      "main.js",
      "package.json",
      "Invoice-management/**/*"
    ],
    "asarUnpack": [
      "Invoice-management/**/*"
    ],
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "nsis"
    },
    "mac": {
      "category": "public.app-category.utilities"
    }
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "electron": "^33.2.1",
    "electron-builder": "^25.1.8"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "electron-is-dev": "^3.0.1",
    "express": "^4.18.2",
    "mongoose": "^7.0.2",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^2.0.22",
    "path-browserify": "^1.0.1",
    "sequelize": "^6.37.5",
    "sqlite3": "^5.1.7"
  }
}
